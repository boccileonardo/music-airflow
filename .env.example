SPOTIFY_ACCESS_TOKEN=
SPOTIFY_REFRESH_TOKEN=

# Google Cloud Storage for Delta tables (optional - defaults to local ./data/)
# GCS_BUCKET_URI=gs://your-bucket-name/data
